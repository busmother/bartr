ate.open\n        }));\n    }\n\n    componentDidMount = () => {\n        this.props.setStatus()\n        this.setState()\n    }\n\n    greeting = () => {\n        console.log (\"this.props from App/greeting\", this.props)\n        if (this.props.user?.status && this.props.user.status === \"resolved\"){\n                return (<h4>Hi {this.props.user.user.username}</h4>)\n            }\n        }\n\n    render(){\n        console.log(\"store from App\", this.store)\n        return(\n            <Router>\n            <div className=\"wrap\">\n                {this.state.open ? <PopUp open={this.state.open} /> : null }  \n                <div className=\"header\">\n                    <h1>Bartr</h1>\n                    {this.greeting()}\n                </div>\n                <nav className=\"nav\">\n                <br></br>\n                <ul className=\"nav-list\">\n                    <li className=\"nav-link\"><Link to=\"/products\">Products </Link></li>\n                    <li className=\"nav-link\"><Link to=\"/cart\">Cart </Link></li>\n                    <li className=\"nav-link\">Checkout</li>\n                    <li className=\"nav-link\"> <Link to=\"/orders\">Past Orders</Link></li>\n                    <li className=\"nav-link\"><Button handleClick={() => this.props.logout()} label = \"Logout\" /></li>\n                </ul>\n                <br></br>\n                </nav>\n                <div className=\"main\">\n                <LoginGate />\n                <Switch>\n                    <Route path = '/login' component = {LoginComponent}/>\n                    <Route path = '/cart' component = {Cart}/>\n                    <Route path = '/products' component = {ProductsContainer}/>\n                    <Route path = '/orders' component = {OrdersContainer}/>\n                </Switch>\n            </div>\n                <aside className=\"sidebar\">\n                <Cart togglePop={this.togglePop}/>\n                </aside>\n                <footer className=\"footer\">\n                    <p></p>\n                </footer>\n            </div>\n            </Router>\n        )\n    }\n}\n\nconst mapDispatchToProps = (dispatch) => {\n    return {\n        logout: () => dispatch({type: 'logout'}),\n        setStatus\n    }\n }\n\n\nexport default connect((state) => {\n    return { user: state.user }\n}, mapDispatchToProps)(App);","/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/Products.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/Cart.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/Button.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/reducers/productReducer.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/actions/fetchProducts.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/ProductsContainer.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/reducers/index.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/reducers/itemReducer.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/reducers/orderReducer.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/Product.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/actions/addItem.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/PopUp.js",["101"],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/Form.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/Login.js",["102","103","104"],"import { useSelector, useDispatch } from 'react-redux'\nimport { login, addUser } from '.././actions/user'\nimport React, { useState, useHistory } from 'react'\n\nexport default function LoginComponent(props) {\n\n    const dispatch = useDispatch();\n    const {username, id} = useSelector(state => state.user);\n  