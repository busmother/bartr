ort './app-stylesheet.css'\nimport ProductsContainer from './components/ProductsContainer'\nimport PopUp from './components/PopUp'\nimport Login from './components/Login'\nimport { connect } from 'react-redux'\nimport { setStatus, logout } from \"./actions/user\"\n\n\nclass App extends Component {\n\n    constructor(props) {\n        super(props);\n        this.state = {\n            open: false,\n            user: {\n                status: \"idle\"\n            }\n        };\n        this.togglePop = this.togglePop.bind(this);\n    }\n\n    togglePop = () => {\n        this.setState(state => ({\n            open: !state.open\n        }));\n    }\n\n    displayLogin = () => {\n\n        if(this.props.user.status === \"idle\"){\n            return <Login></Login>\n        }if(this.props.user.status === \"pending\"){\n            return <div>Loading...</div>\n        }if(this.props.user.status === \"resolved\"){\n            return (\n            <div>\n                <p>\n                Welcome {this.props.username}\n                </p>\n                <button onClick={this.logout}>Logout</button>\n            </div>\n            );\n        }if(this.props.user.status === \"rejected\"){\n            return <div>Oops you're not logged in</div>\n        }else{\n            return <div>An unforeseen misfortune has occured.</div>\n        }\n    }\n\n    componentDidMount = () => {\n        this.props.setStatus()\n        this.setState()\n        console.log(\"state from App\", this.state)\n\n    }\n\n    render(){\n        console.log(\"this.props.user\", this.props.user)\n\n        return(\n            <Router>\n            <div className=\"wrap\">\n                {this.state.open ? <PopUp open={this.state.open} /> : null }  \n                <div className=\"header\">\n                    <h1>Bartr</h1>\n                    {/* <h2>Using the method: {this.sayHi()}</h2>  */}\n                    <h2>Hi {this.props.user.username}</h2>\n                    {/* ^this only updates when you refresh, so this info needs a condition and componentDidUpdate */}\n                </div>\n                <nav className=\"nav\">\n                <br></br>\n                <ul className=\"nav-list\">\n                    <li className=\"nav-link\"><Link to=\"/\">Products </Link></li>\n                    <li className=\"nav-link\"><Link to=\"/cart\">Cart </Link></li>\n                    <li className=\"nav-link\">Checkout</li>\n                    <li className=\"nav-link\"> Past Orders </li>\n                    <li className=\"nav-link\"><Link to=\"/login\">Switch User</Link></li>\n                </ul>\n                <br></br>\n                </nav>\n                <div className=\"main\">\n                <Switch>\n                    <Route path = '/login' component = {Login}/>\n                    <Route path = '/cart' component = {Cart}/>\n                    <Route path = '/' component = {ProductsContainer}/>\n                </Switch>\n            </div>\n                <aside className=\"sidebar\">\n                <Cart togglePop={this.togglePop}/>\n                </aside>\n                <footer className=\"footer\">\n                    <p></p>\n                </footer>\n            </div>\n            </Router>\n        )\n    }\n}\n\nexport default connect((state) => {\n    return { user: state.user }\n}, {setStatus})(App);","/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/Products.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/Cart.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/Button.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/reducers/productReducer.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/actions/fetchProducts.js",[],"/Users/ianmarshall/Development/phase5/bartr/bartr-frontend/src/components/ProductsContainer.js",[],"/Users/ianmarshall/Development/phase5/bartr/b